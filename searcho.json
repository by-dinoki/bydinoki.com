---
layout: none
---

[
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title | xml_escape }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date | localize: '%d de %B del %Y', 'es' | xml_escape }}",
      "description" : "{{ post.description | xml_escape }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]